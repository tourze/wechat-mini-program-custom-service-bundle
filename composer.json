{
  "name": "tourze/wechat-mini-program-custom-service-bundle",
  "description": "微信小程序客服消息服务包，提供发送文本、图片、链接和小程序卡片消息的功能",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "doctrine/dbal": "^4.0",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/orm": "^3.0",
    "doctrine/persistence": "^4.1",
    "easycorp/easyadmin-bundle": "^4",
    "knplabs/knp-menu": "^3.7",
    "symfony/config": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/doctrine-bridge": "^7.3",
    "symfony/event-dispatcher": "^7.3",
    "symfony/framework-bundle": "^7.4",
    "symfony/http-foundation": "^7.4",
    "symfony/http-kernel": "^7.3",
    "symfony/property-access": "^7.3",
    "symfony/routing": "^7.3",
    "symfony/yaml": "^7.3",
    "tourze/bundle-dependency": "1.*",
    "tourze/easy-admin-menu-bundle": "1.0.*",
    "tourze/symfony-dependency-service-loader": "1.0.*",
    "tourze/symfony-routing-auto-loader-bundle": "1.0.*",
    "tourze/wechat-mini-program-bundle": "1.0.*"
  },
  "require-dev": {
    "doctrine/data-fixtures": "^2.0",
    "doctrine/doctrine-fixtures-bundle": "^4.0",
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "tourze/http-client-bundle": "1.1.*",
    "tourze/phpunit-doctrine-entity": "1.*",
    "tourze/phpunit-symfony-kernel-test": "2.0.*",
    "tourze/phpunit-symfony-unit-test": "2.0.*",
    "tourze/phpunit-symfony-web-test": "2.0.*"
  },
  "autoload": {
    "psr-4": {
      "WechatMiniProgramCustomServiceBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WechatMiniProgramCustomServiceBundle\\Tests\\": "tests"
    }
  }
}